<%= stylesheet_link_tag "index.css" %>
<h1>Buck$$</h1>

 
<%= link_to 'Create New Task', new_listing_path  %>


<div class="listings-all">
      <% @listings.each do |listing| %>

  <ul class="listing-item">
  <li>Industry: <%=listing.industry %></li>
  <li>Task: <%=listing.task %></li>
  <li>Bucks: <%=number_to_currency(listing.price)%></li>
  <li><%= link_to 'View Task', listing_path(listing)%></li>
  </ul>
 <% end %>
</div>




